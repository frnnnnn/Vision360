# AWS Credentials (get from AWS Academy or IAM)
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_SESSION_TOKEN=your_session_token_here
AWS_DEFAULT_REGION=us-east-1

# AWS Resources (create these in your AWS account)
S3_BUCKET=your-bucket-name
DDB_TABLE=your-dynamodb-table
SNS_TOPIC_ARN=arn:aws:sns:us-east-1:YOUR_ACCOUNT_ID:your-topic

# Camera Configuration
CAMERA_ID=cam01
CAMERA_URL=http://your-camera-ip:8081/video

# Face Recognition (optional)
FACE_COLLECTION_ID=vision360-faces
FACE_MATCH_THRESHOLD=80
